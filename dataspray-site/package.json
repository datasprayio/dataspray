{
  "name": "dataspray",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev:dev": "next dev",
    "dev": "concurrently -n NX,TS -c cyan,green \"next dev\" \"pnpm dev:ts --watch\"",
    "dev:ts": "tsc --noEmit --incremental --preserveWatchOutput --pretty",
    "build": "pnpm run build:staticsite",
    "build:opennext": "NEXTJS_OUTPUT=standalone open-next build --build-command 'pnpm run build:next'",
    "build:next": "next build",
    "build:staticsite": "NEXTJS_OUTPUT=export pnpm run build:next",
    "start": "pnpm run start:staticsite",
    "start:staticsite": "serve out",
    "start:next": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.485.0",
    "@cloudscape-design/board-components": "^3.0.49",
    "@cloudscape-design/components": "^3.0.478",
    "@cloudscape-design/components-themeable": "^3.0.494",
    "@cloudscape-design/global-styles": "^1.0.21",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mdx-js/loader": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "@next/mdx": "^14.0.4",
    "@types/mdx": "^2.0.10",
    "@types/node": "^17.0.45",
    "@types/node-fetch": "^2.6.10",
    "formik": "^2.4.5",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "concurrently": "^8.2.2",
    "esbuild": "^0.18.20",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "inquirer": "^9.2.12",
    "open-next": "^2.3.4",
    "serve": "^14.2.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0"
  },
  "pnpm": {
    "patchedDependencies": {
      "@cloudscape-design/components@3.0.478": "patches/@cloudscape-design__components@3.0.478.patch"
    }
  }
}