{
  "name": "dataspray",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:dev": "next dev",
    "dev": "concurrently -n NX,TS -c cyan,green \"next dev\" \"yarn dev:ts --watch\"",
    "dev:ts": "tsc --noEmit --incremental --preserveWatchOutput --pretty",
    "build": "pnpm run build:opennext",
    "build:opennext": "open-next build --build-command 'pnpm run build:next'",
    "build:next": "next build && next export",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@fontsource/roboto": "^4.5.5",
    "@mdx-js/loader": "^2.1.1",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.9.0",
    "@next/mdx": "^12.1.6",
    "next": "13.4.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "vscode-web": "^1.69.2"
  },
  "devDependencies": {
    "@sls-next/serverless-component": "^3.7.0",
    "@sls-next/serverless-patched": "^3.4.0",
    "@types/node": "17.0.31",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "concurrently": "^7.4.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "8.15.0",
    "eslint-config-next": "13.4.9",
    "open-next": "^2.0.4",
    "serverless": "^2.72.3",
    "typescript": "4.6.4"
  },
  "pnpm": {
    "patchedDependencies": {
      "@serverless/cli@1.6.0": "patches/@serverless__cli@1.6.0.patch",
      "@sls-next/serverless-component@3.7.0": "patches/@sls-next__serverless-component@3.7.0.patch"
    }
  }
}