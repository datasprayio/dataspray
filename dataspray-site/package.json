{
  "name": "dataspray",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev:dev": "next dev",
    "dev": "concurrently -n NX,TS -c cyan,green \"next dev\" \"pnpm dev:ts --watch\"",
    "dev:ts": "tsc --noEmit --incremental --preserveWatchOutput --pretty",
    "build": "pnpm run build:staticsite",
    "build:opennext": "NEXTJS_OUTPUT=standalone open-next build --build-command 'pnpm run build:next'",
    "build:next": "next build",
    "build:staticsite": "NEXTJS_OUTPUT=export pnpm run build:next",
    "start": "pnpm run start:staticsite",
    "start:staticsite": "serve out",
    "start:next": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.465.0",
    "@cloudscape-design/board-components": "^3.0.43",
    "@cloudscape-design/components": "^3.0.443",
    "@cloudscape-design/components-themeable": "^3.0.456",
    "@cloudscape-design/global-styles": "^1.0.15",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.5",
    "@types/node": "^17.0.45",
    "@types/node-fetch": "^2.6.9",
    "formik": "^2.4.5",
    "next": "^14.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "concurrently": "^8.2.0",
    "esbuild": "^0.18.17",
    "eslint": "^8.46.0",
    "eslint-config-next": "^14.0.3",
    "inquirer": "^9.2.8",
    "open-next": "^2.3.2",
    "serve": "^14.2.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2"
  },
  "pnpm": {
    "patchedDependencies": {
      "@cloudscape-design/components@3.0.443": "patches/@cloudscape-design__components@3.0.443.patch"
    }
  }
}