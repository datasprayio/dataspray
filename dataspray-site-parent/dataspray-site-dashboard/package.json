{
  "name": "dataspray",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev:dev": "next dev",
    "dev": "concurrently -n NX,TS -c cyan,green \"next dev\" \"pnpm dev:ts --watch\"",
    "dev:ts": "tsc --noEmit --incremental --preserveWatchOutput --pretty",
    "build": "pnpm run build:staticsite",
    "build:opennext": "NEXTJS_OUTPUT=standalone open-next build --build-command 'pnpm run build:next'",
    "build:next": "next build",
    "build:staticsite": "NEXTJS_OUTPUT=export pnpm run build:next",
    "start": "pnpm run start:staticsite",
    "start:staticsite": "serve out",
    "start:next": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.738.0",
    "@cloudscape-design/board-components": "^3.0.89",
    "@cloudscape-design/collection-hooks": "^1.0.59",
    "@cloudscape-design/components": "3.0.1164",
    "@cloudscape-design/components-themeable": "^3.0.892",
    "@cloudscape-design/global-styles": "^1.0.35",
    "@fontsource/roboto": "^5.1.1",
    "@types/node": "^22.12.0",
    "@types/node-fetch": "^2.6.12",
    "dataspray-client": "workspace:*",
    "formik": "^2.4.6",
    "jwt-decode": "^4.0.0",
    "next": "^15.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "swr": "^2.3.0",
    "yup": "^1.6.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "concurrently": "^9.1.2",
    "esbuild": "^0.24.2",
    "eslint": "^9.19.0",
    "eslint-config-next": "^15.1.6",
    "inquirer": "^12.3.3",
    "open-next": "^3.1.3",
    "serve": "^14.2.4",
    "typescript": "^5.7.3",
    "webpack": "^5.97.1"
  }
}